  "dependencies": {
    "@raycast/api": "^1.91.1",
    "@raycast/utils": "^1.17.0",
    "@vitalets/google-translate-api": "^9.2.1",
    "axios": "^1.7.9",
    "https-proxy-agent": "^7.0.6",
    "lodash": "^4.17.21",
