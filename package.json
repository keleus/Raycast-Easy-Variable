  "icon": "extension-icon.png",
  "author": "chs",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "preferences": [
    {
      "type": "textfield",
      "title": "Http Proxy",
      "description": "Http Proxy URL",
